<!--
..[eth1]S11[eth2]...4....[eth2]S1[eth3].....3.....[eth2]S2[eth4]...2...[eth2]S12[eth1]..
                             [eth4]                   [eth3]
                               |1                       |0
                             [eth2]                     |
                               S3[eth3].................'

SW              ITR
S1[eth1] ... [eth0]t11 - 5
S2[eth1] ... [eth0]t21 - 6
S3[eth1] ... [eth0]t31 - 7

Caps
2-4: wide-link-opts=10Mbps #100Mbps, 50ms
0-1-3: local-link-opts=200Mbps #200Mbps, 5ms
-->
<networks>
	<network number="1">
		<nodes>
			<s11 type="sw" dpid="11"  out_bw="0" in_bw="0"/>
			<s1  type="sw" dpid="1"   out_bw="0" in_bw="0"/>
			<s2  type="sw" dpid="2"   out_bw="0" in_bw="0"/>
			<s3  type="sw" dpid="3"   out_bw="0" in_bw="0"/>
			<s12 type="sw" dpid="12"  out_bw="0" in_bw="0"/>
			<t11 type="t" ip="10.0.0.11" mac="00:00:00:00:01:01" p_index="0.01" proc_cap="300" stor_cap="600" session=""/>
			<t21 type="t" ip="10.0.0.21" mac="00:00:00:00:02:01" p_index="0.01" proc_cap="300" stor_cap="600" session=""/>
			<t31 type="t" ip="10.0.0.31" mac="00:00:00:00:03:01" p_index="0.01" proc_cap="300" stor_cap="600" session=""/>
		</nodes>
		<edges>
		  <!-- SW...SW-->
			<edge pre_node="s11" post_node="s1">
			  <dev pre_dev="s11-eth2" post_dev="s1-eth2"/>
			  <session/>
			  <link_cap bw="10" delay="50" loss="0" max_queue_size="1000"/>
			</edge>
			<edge pre_node="s1" post_node="s2">
			  <dev pre_dev="s1-eth3" post_dev="s2-eth2"/>
			  <session/>
			  <link_cap bw="10" delay="5" loss="0" max_queue_size="1000"/>
			</edge>
			<edge pre_node="s1" post_node="s3">
			  <dev pre_dev="s1-eth4" post_dev="s3-eth2"/>
			  <session/>
			  <link_cap bw="10" delay="5" loss="0" max_queue_size="1000"/>
			</edge>
			<edge pre_node="s3" post_node="s2">
			  <dev pre_dev="s3-eth3" post_dev="s2-eth3"/>
			  <session/>
			  <link_cap bw="10" delay="5" loss="0" max_queue_size="1000"/>
			</edge>
			<edge pre_node="s2" post_node="s12">
			  <dev pre_dev="s2-eth4" post_dev="s12-eth2"/>
			  <session/>
			  <link_cap bw="10" delay="50" loss="0" max_queue_size="1000"/>
			</edge>
			<!-- SW...ITR -->
			<edge pre_node="s1" post_node="t11">
			  <dev pre_dev="s1-eth1" post_dev="t11-eth0"/>
			  <session/>
			  <link_cap bw="100" delay="0" loss="0" max_queue_size="10000"/>
			</edge>
			<edge pre_node="s2" post_node="t21">
			  <dev pre_dev="s2-eth1" post_dev="t21-eth0"/>
			  <session/>
			  <link_cap bw="100" delay="0" loss="0" max_queue_size="10000"/>
			</edge>
			<edge pre_node="s3" post_node="t31">
			  <dev pre_dev="s3-eth1" post_dev="t31-eth0"/>
			  <session/>
			  <link_cap bw="100" delay="0" loss="0" max_queue_size="10000"/>
			</edge>
	  </edges>
	</network>
</networks>
